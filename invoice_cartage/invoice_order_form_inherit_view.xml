<?xml version="1.0" encoding="utf-8"?><openerp>    <data>          <record id="account_cashs_sequencs" model="ir.sequence">            <field name="name">cashs</field>            <field name="code">account.invoice.cashs</field>            <field name="prefix">CM/%(year)s/</field>            <field name="padding">5</field>            <field name="domain">[('serial_number','=',cash_invoice)]</field>        </record>         <record id="account_credits_sequence" model="ir.sequence">            <field name="name">credits</field>            <field name="code">account.invoice.credits</field>            <field name="prefix">CRM/%(year)s/</field>            <field name="padding">5</field>            <field name="domain">[('serial_number','=',credit_invoice)]</field>        </record>            <record id="invoice_form_view" model="ir.ui.view">            <field name="name">account.invoice.form</field>            <field name="model">account.invoice</field>            <field name="inherit_id" ref="account.invoice_form" />            <field name="arch" type="xml">               <data>                    <field name="comment" position="before">                               <group colspan="1" rowspan="0">                    <field name="cash_credit" style="width:167px;" required="True"/>                    <field name="cartage" sum="Cartage Amount"/>                    <field name="labour" sum="Labour Amount"/>                     </group>                                        </field>                    <field name="date_invoice" position="before">                        <field name="serial_number"/>                    </field>                                            <!-- <field name="comment" position="before">                                <field name="cartage" sum="Cartage Amount"/>                                <field name="labour" sum="Labour Amount"/>                                                </field> -->                                        </data>            </field>            </record><record id="view_account_invoice_filter" model="ir.ui.view">            <field name="name">account.invoice.select.inherit</field>            <field name="model">account.invoice</field>            <field name="inherit_id" ref="account.view_account_invoice_filter" />            <field name="arch" type="xml">               <data>					<!-- <group name="sort_by_cash_credit" position="before">						<field name="cash_credit">						</field>						<field name ="amount_tax">						</field>					</group> -->                </data>            </field>        </record>        <record id="invoice_tree" model="ir.ui.view">        <field name="name">account.invoice.tree.inherit</field>        <field name="model">account.invoice</field>        <field name="inherit_id" ref="account.invoice_tree" />        <field name="arch" type="xml">            <data>				<field name="number" position="after">					<field name="cash_credit"/>				</field>					<!-- <field name="payment_term" position="before">						<field name="cartage" sum="Cartage Amount"/>					</field> -->                </data>            </field>        </record></data></openerp>