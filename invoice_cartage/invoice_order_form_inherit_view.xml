<?xml version="1.0" encoding="utf-8"?><openerp>    <data>          <record id="account_cashs_sequencs" model="ir.sequence">            <field name="name">cashs</field>            <field name="code">account.invoice.cashs</field>            <field name="prefix">CM/%(year)s/</field>            <field name="padding">5</field>            <field name="domain">[('serial_number','=',cash_invoice)]</field>        </record>         <record id="account_credits_sequence" model="ir.sequence">            <field name="name">credits</field>            <field name="code">account.invoice.credits</field>            <field name="prefix">CRM/%(year)s/</field>            <field name="padding">5</field>            <field name="domain">[('serial_number','=',credit_invoice)]</field>        </record>            <record id="invoice_form_view" model="ir.ui.view">            <field name="name">account.invoice.form</field>            <field name="model">account.invoice</field>            <field name="inherit_id" ref="account.invoice_form" />            <field name="arch" type="xml">               <data>                   <button name="invoice_print" position="after">                       <button name="invoice_print_custom" type="object" string="Custom Print"/>                   </button>                   <field name="comment" position="before">                    <group colspan="1" rowspan="0">                    <field name="cash_credit" style="width:167px;" required="True"/>                    <field name="cartage" sum="Cartage Amount" attrs="{'readonly':[('state','not in',('draft','proforma','proforma2'))]}"/>                    <field name="labour" sum="Labour Amount" attrs="{'readonly':[('state','not in',('draft','proforma','proforma2'))]}"/>                     </group>                                        </field>                    <field name="date_invoice" position="before">                        <field name="serial_number"/>                    </field>                                            <!-- <field name="comment" position="before">                                <field name="cartage" sum="Cartage Amount"/>                                <field name="labour" sum="Labour Amount"/>                                                </field> -->                                        </data>            </field>            </record>        <record id="view_account_invoice_filters" model="ir.ui.view">        <field name="name">account.invoice.select</field>        <field name="model">account.invoice</field>        <field name="inherit_id" ref="account.view_account_invoice_filter"/>        <field name="arch" type="xml">            <search string="Search Invoice">               <data>					                </data>            </search>            </field>        </record>        <record id="invoice_tree" model="ir.ui.view">        <field name="name">account.invoice.tree.inherit</field>        <field name="model">account.invoice</field>        <field name="inherit_id" ref="account.invoice_tree" />        <field name="arch" type="xml">            <data>				<field name="number" position="after">					<field name="cash_credit"/>				</field>					<!-- <field name="payment_term" position="before">						<field name="cartage" sum="Cartage Amount"/>					</field> -->                </data>            </field>        </record>        <report            id="account_invoices1"            model="account.invoice"            string="Daily Cash Report"            report_type="qweb-pdf"            name="invoice_cartage.report_invoice_list"            file="invoice_cartage.report_invoice_list"            attachment_use="True"            attachment="(object.state in ('open','paid')) and ('INV'+(object.number or '').replace('/','')+'.pdf')"        />        <template id="report_invoice_list">            <t t-call="report.html_container">                 <h2>                    <span style="align:center">Daily Cash Report</span>                </h2>                <table class="table table-condensed">                <t t-foreach="docs" t-as="o">                        <thead style="background-color:black;color:white" t-if="o_index ==0">                            <tr style="border:1px solid #000000">                                <th>Date</th>                                <th>Particular</th>                                <th>Narration</th>                                <th class="text-right">Gross Total</th>                                <th class="text-right">Sales 14.5 %</th>                                <th class="text-right">Vat 14.5 %</th>                                <th class="text-right">Sales 5 %</th>                                <th class="text-right">Vat 5 %</th>                                <th class="text-right">Sales Others</th>                                <th class="text-right">Others</th>                                <th class="text-right">Cartage</th>                                <th class="text-right">Labour</th>                            </tr>                        </thead>                        <tbody class="invoice_tbody">                            <tr t-foreach="o.invoice_line_ids" t-as="l" style="border:1px solid #000000">                                <td style="border:1px solid #000000"><span t-field="o.date_invoice"/></td>                                <td style="border:1px solid #000000">                                    <span t-field="o.partner_id.name"/>                                </td>                                <td class="text-right" style="border:1px solid #000000">                                    <span t-esc="o.number"/>                                </td>                                <td class="text-right" style="border:1px solid #000000">                                    <span t-field="l.price_subtotal"                                        t-field-options='{"widget": "monetary", "display_currency": "o.currency_id"}'/>                                </td>                                <td class="text-right" style="border:1px solid #000000">                                    <t t-foreach="o.tax_line_ids" t-as="t">                                        <t t-if="t.name=='14.5' and t.tax_id.price_include">                                            <span t-field="t.amount"/>                                        </t>                                    </t>                                </td>                                <td class="text-right" style="border:1px solid #000000">                                   <t t-foreach="o.tax_line_ids" t-as="t">                                        <t t-if="t.name=='14.5E' and not t.tax_id.price_include">                                            <span t-field="t.amount"/>                                        </t>                                    </t>                                </td>                                <td class="text-right" style="border:1px solid #000000">                                    <t t-foreach="o.tax_line_ids" t-as="t">                                        <t t-if="t.name=='5'">                                            <t t-esc="t.amount"/>                                        </t>                                    </t>                                </td>                                <td class="text-right" style="border:1px solid #000000">                                    <t t-foreach="o.tax_line_ids" t-as="t">                                        <t t-if="t.name=='5E'">                                            <t t-esc="t.amount"/>                                        </t>                                    </t>                                </td>                                <td class="text-right" style="border:1px solid #000000">                                    <t t-set="total" t-value="0.0"/>                                    <t t-foreach="o.tax_line_ids" t-as="t1">                                        <t t-if="t1.name not in ('5E','5','14.5','14.5E') and t1.tax_id.price_include">                                            <t t-set="total" t-value="total+ t1.amount"/>                                        </t>                                    </t>                                    <t t-esc="total"/>                                </td>                                <td class="text-right" style="border:1px solid #000000">                                    <t t-set="total" t-value="0.0"/>                                    <t t-foreach="o.tax_line_ids" t-as="t2">                                        <t t-if="t2.name not in ('5E','5','14.5','14.5E') and not t2.tax_id.price_include">                                            <t t-set="total" t-value="total+ t2.amount"/>                                        </t>                                    </t>                                    <t t-esc="total"/>                                </td>                                <td class="text-right" style="border:1px solid #000000">                                    <span t-esc="o.cartage"/>                                </td>                                <td class="text-right" style="border:1px solid #000000">                                    <span t-esc="o.labour"/>                                </td>                            </tr>                        </tbody>                    </t>                </table>            </t>        </template></data></openerp>